{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"crons": [
		{
			"path": "/api/populate",
			"schedule": "10 8 * * *"
		}
	],
	"rewrites": [
		{
			"source": "/api/ingest/static/:path(.*)",
			"destination": "https://us-assets.i.posthog.com/static/:path"
		},
		{
			"source": "/api/ingest/:path(.*)",
			"destination": "https://us.i.posthog.com/:path"
		}
	]
}
